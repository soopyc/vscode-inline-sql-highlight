{
	"name": "vscode-inline-sql-highlight",
	"displayName": "Inline SQL Highlight",
	"description": "Highlights the SQL queries from your code",
	"version": "0.2.1",
	"publisher": "soopyc",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/soopyc/vscode-inline-sql-highlight.git"
	},
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"source.js",
					"source.ts",
					"source.go"
				],
				"scopeName": "inline.sql",
				"path": "./syntaxes/inline-sql.json",
				"embeddedLanguages": {
					"meta.embedded.block.sql": "sql"
				}
			}
		]
	}
}
